<launch>
  <arg name="ip_address" default="192.168.1.140" />
  <arg name="port" default="63352" />

  <node name="robotiq_2f_85_node" pkg="robotiq_2f_85_control" type="robotiq_2f_85_modbus_tcp_node.py" output="screen">
    <param name="ip_address" value="$(arg ip_address)" />
    <param name="port" value="$(arg port)" />
  </node>
</launch>